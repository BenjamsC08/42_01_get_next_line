char	*get_next_line(int fd)
{
	static int	n = 0;
	int			*tab;
	int			start;
	char		*str;

	tab = count_char(fd);
	if (tab[n + 1] == -1)
	{
		free(tab);
		return (NULL);
	}
	str = (char *)malloc(tab[n + 1] - tab[n]);
	if (!str)
		return (NULL);
	if (n == 0)
		start = 0;
	else
		start = tab[n] - tab[n - 1];
	while (start < tab[n + 1])
		str[start] = buff[start + tab[n]];
	free(tab);
	n++;
	return (NULL);
}
